<script type="text/javascript">
  $(function() {
    var hash = window.location.hash.substr(1);
    if(hash.length) {
      $('#login_form').append('<input type="hidden" name="hash" value="' + htmlspecialchars(hash) + '" />');
    }

    if(isMobileDevice()) {
      $('#login_form').attr('novalidate', '');
      $('#login_login').prop('type', 'email');
    }

    $('[data-autofocus]').focus();
  });

  $(document).ready(function() {
    $(window).on("capsOn", function() {
      if($("#login_password:focus").length > 0) {
        $("#capsWarning").removeClass('hidden');
      }
    });

    $(window).on("capsOff capsUnknown", function() {
      $("#capsWarning").addClass('hidden');
    });

    $("#login_password").on("focusout", function() {
      $("#capsWarning").addClass('hidden');
    });

    $("#login_password").on("focusin", function() {
      if ($(window).capslockstate("state") === true) {
        $("#capsWarning").removeClass('hidden');
      }
    });

    $(window).capslockstate();
  });
</script>

